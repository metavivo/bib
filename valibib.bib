@article{Wagner_Arkin2010,
	Abstract = {Deception is utilized by a variety of intelligent systems ranging from insects to human beings. It has been argued that the use of deception is an indicator of theory of mind (Cheney and Seyfarth in Baboon Metaphysics: The Evolution of a Social Mind, 2008) and of social intelligence (Hauser in Proc. Natl. Acad. Sci. 89:12137--12139, 1992). We use interdependence theory and game theory to explore the phenomena of deception from the perspective of ro- botics, and to develop an algorithm which allows an arti- ficially intelligent system to determine if deception is war- ranted in a social situation. Using techniques introduced in Wagner (Proceedings of the 4th International Conference on Human-Robot Interaction (HRI 2009), 2009), we present an algorithm that bases a robot's deceptive action selec- tion on its model of the individual it's attempting to de- ceive. Simulation and robot experiments using these algo- rithms which investigate the nature of deception itself are discussed.},
	Annote = {Primo articolo, con grande risonanza, su robot che ingannano. Utile per humor e irony},
	Author = {Alan R. Wagner and Ronald C. Arkin},
	Date-Added = {2010-09-15 16:14:28 +0100},
